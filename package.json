{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant devDebug --appIdSuffix=dev",
    "ios": "react-native run-ios",
    "macos": "react-native run-macos",
    "windows": "react-native run-windows",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .js ./src",
    "packager": "env watchman watch-del-all && npm start -- --reset-cache",
    "packager:macos": "node node_modules/react-native-macos/local-cli/cli.js start --use-react-native-macos",
    "packager:windows": "react-native start -- --use-react-native-windows"
  },
  "dependencies": {
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.1.1",
    "axios": "^0.20.0",
    "formik": "^2.1.4",
    "react": "16.13.0",
    "react-native": "0.61.5",
    "react-native-macos": "^0.61.0-0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "yup": "^0.28.3",
    "react-native-windows": "^0.61.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@react-native-community/eslint-config": "^0.0.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-react-native": "^3.8.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-test-renderer": "16.13.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "jest": {
    "preset": "react-native"
  }
}
